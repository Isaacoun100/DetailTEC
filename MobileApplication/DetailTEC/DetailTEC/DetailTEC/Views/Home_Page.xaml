<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DetailTEC.Views.Home_Page">
    <ContentPage.Content>
        <StackLayout>
            <Image Source="logo_2.png" HorizontalOptions="Center" VerticalOptions="Center" />
            <Button x:Name="appointmentbtn" Text="Make an appointment" Grid.Row="1" Clicked="Go_Appointment"/>
            <Button x:Name="billsbtn" Text="Check points" Clicked="Go_Points" Grid.Row="2"/>
            <Button x:Name="userbtn" Text="My profile" Clicked="Go_Profile" Grid.Row="3"/>
            <Button x:Name="dbsync" Text="Sync Database" Clicked="Go_DBSync" Grid.Row="3"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>